    "typescript": "~3.7.2"
  },
  "scripts": {
    "start:server": "cd server && ts-node-dev --respawn --transpile-only ./server.ts && wait-on tcp:5000",
    "start:app": "react-scripts start",
    "start": "run-p --race start:server start:app",
    "deploy": "gcloud app deploy PROD/app.yaml",
    "build": "react-scripts build && mkdir PROD && mv build PROD/build && tsc -p server/ && cp package.prod.json PROD/package.json && cp app.yaml PROD/app.yaml ",
    "test": "CI=true react-scripts test ",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.7.0"
  }
